{
    "name": "repo-maker",
    "description": "Web Application create a skeleton repo based on your stack",
    "version": "1.0.0",
    "main": "server/server.js",
    "devDependencies": {},
    "scripts": {
        "install": "cd server && npm i && cd ../client && npm i",
        "build": "cd client && npm run build",
        "dev": "concurrently \"cd server && npm run watch\" \"cd client && npm start\"",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Proto133/repo-maker.git"
    },
    "author": "Peter Roto",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Proto133/repo-maker/issues"
    },
    "homepage": "https://github.com/Proto133/repo-maker#readme",
    "dependencies": {
        "concurrently": "^6.2.1"
    }
}
